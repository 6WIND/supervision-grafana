{
  "type": "timeseries",
  "title": "Linux CPUs",
  "gridPos": {
    "x": 12,
    "y": 1,
    "h": 7,
    "w": 6
  },
  "span": 3,
  "fieldConfig": {
    "defaults": {
      "custom": {
        "drawStyle": "line",
        "lineInterpolation": "linear",
        "barAlignment": 0,
        "barWidthFactor": 0.6,
        "lineWidth": 1,
        "fillOpacity": 10,
        "gradientMode": "none",
        "spanNulls": false,
        "insertNulls": false,
        "showPoints": "never",
        "pointSize": 5,
        "stacking": {
          "mode": "none",
          "group": "A"
        },
        "axisPlacement": "auto",
        "axisLabel": "",
        "axisColorMode": "text",
        "axisBorderShow": false,
        "scaleDistribution": {
          "type": "linear"
        },
        "axisCenteredZero": false,
        "hideFrom": {
          "tooltip": false,
          "viz": false,
          "legend": false
        },
        "thresholdsStyle": {
          "mode": "off"
        }
      },
      "color": {
        "mode": "palette-classic"
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 80
          }
        ]
      },
      "max": 100,
      "min": 0,
      "unit": "percent"
    },
    "overrides": []
  },
  "pluginVersion": "12.1.0",
  "targets": [
    {
      "alias": "$tag_cpu",
      "datasource": {
        "type": "influxdb"
      },
      "dsType": "influxdb",
      "groupBy": [
        {
          "params": [
            "cpu::tag"
          ],
          "type": "tag"
        }
      ],
      "measurement": "system-cpu-usage",
      "orderByTime": "ASC",
      "policy": "default",
      "refId": "A",
      "resultFormat": "time_series",
      "select": [
        [
          {
            "params": [
              "busy"
            ],
            "type": "field"
          }
        ]
      ],
      "tags": [
        {
          "key": "host::tag",
          "operator": "=~",
          "value": "/^$host$/"
        },
        {
          "condition": "AND",
          "key": "cpu::tag",
          "operator": "!~",
          "value": "/^$fpcpu$/"
        }
      ],
      "query": "SELECT \"busy\" FROM \"system-cpu-usage\" WHERE (\"host\"::tag =~ /^$host$/ AND \"cpu\"::tag !~ /^$fpcpu$/) AND $timeFilter GROUP BY \"cpu\"::tag",
      "rawQuery": true
    },
    {
      "alias": "mean (1min)",
      "datasource": {
        "type": "influxdb"
      },
      "dsType": "influxdb",
      "groupBy": [
        {
          "params": [
            "1m"
          ],
          "type": "time"
        }
      ],
      "measurement": "system-cpu-usage",
      "orderByTime": "ASC",
      "policy": "default",
      "refId": "B",
      "resultFormat": "time_series",
      "select": [
        [
          {
            "params": [
              "busy"
            ],
            "type": "field"
          },
          {
            "params": [],
            "type": "mean"
          }
        ]
      ],
      "tags": [
        {
          "key": "host::tag",
          "operator": "=~",
          "value": "/^$host$/"
        },
        {
          "condition": "AND",
          "key": "cpu::tag",
          "operator": "!~",
          "value": "/^$fpcpu$/"
        }
      ]
    }
  ],
  "datasource": {
    "type": "influxdb"
  },
  "options": {
    "tooltip": {
      "mode": "multi",
      "sort": "none",
      "hideZeros": false
    },
    "legend": {
      "showLegend": true,
      "displayMode": "list",
      "placement": "bottom",
      "calcs": []
    }
  }
}
