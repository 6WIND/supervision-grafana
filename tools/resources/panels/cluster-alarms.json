{
  "type": "timeseries",
  "title": "Raised alarms",
  "gridPos": {
    "x": 3,
    "y": 0,
    "h": 8,
    "w": 4
  },
  "fieldConfig": {
    "defaults": {
      "custom": {
        "drawStyle": "line",
        "lineInterpolation": "linear",
        "barAlignment": 0,
        "barWidthFactor": 0.6,
        "lineWidth": 1,
        "fillOpacity": 11,
        "gradientMode": "none",
        "spanNulls": false,
        "insertNulls": false,
        "showPoints": "auto",
        "pointSize": 5,
        "stacking": {
          "mode": "none",
          "group": "A"
        },
        "axisPlacement": "auto",
        "axisLabel": "",
        "axisColorMode": "text",
        "axisBorderShow": false,
        "scaleDistribution": {
          "type": "linear"
        },
        "axisCenteredZero": false,
        "hideFrom": {
          "tooltip": false,
          "viz": false,
          "legend": false
        },
        "thresholdsStyle": {
          "mode": "off"
        },
        "lineStyle": {
          "fill": "solid"
        }
      },
      "color": {
        "mode": "palette-classic"
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          }
        ]
      }
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "allowed"
        },
        "properties": [
          {
            "id": "custom.lineStyle",
            "value": {
              "dash": [
                10,
                10
              ],
              "fill": "dash"
            }
          },
          {
            "id": "custom.fillOpacity",
            "value": 0
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "tmp"
        },
        "properties": [
          {
            "id": "custom.hideFrom",
            "value": {
              "legend": true,
              "tooltip": true,
              "viz": true
            }
          }
        ]
      }
    ]
  },
  "transformations": [
    {
      "id": "calculateField",
      "options": {
        "alias": "tmp",
        "mode": "reduceRow",
        "reduce": {
          "include": [],
          "reducer": "sum"
        },
        "replaceFields": false
      }
    },
    {
      "id": "calculateField",
      "options": {
        "alias": "used $cluster",
        "binary": {
          "left": {
            "matcher": {
              "id": "byName",
              "options": "tmp"
            }
          },
          "operator": "-",
          "right": {
            "matcher": {
              "id": "byName",
              "options": "allowed"
            }
          }
        },
        "cumulative": {
          "field": "Total",
          "reducer": "sum"
        },
        "mode": "binary",
        "reduce": {
          "reducer": "sum"
        }
      }
    }
  ],
  "pluginVersion": "12.0.2",
  "targets": [
    {
      "alias": "$tag_host",
      "datasource": {
        "type": "influxdb"
      },
      "groupBy": [
        {
          "params": [
            "10s"
          ],
          "type": "time"
        },
        {
          "params": [
            "host::tag"
          ],
          "type": "tag"
        },
        {
          "params": [
            "linear"
          ],
          "type": "fill"
        }
      ],
      "measurement": "alarm-summary",
      "orderByTime": "ASC",
      "policy": "autogen",
      "query": "SELECT (last(\"pppoe-sessions-current\") + last(\"ipoe-sessions-current\"))  AS \"used\" FROM \"autogen\".\"fp-filling\" WHERE \"cluster\"::tag =~ /^$cluster$/ AND $timeFilter GROUP BY time(10s), \"host\"::tag fill(linear)",
      "rawQuery": false,
      "refId": "A",
      "resultFormat": "time_series",
      "select": [
        [
          {
            "params": [
              "not-cleared"
            ],
            "type": "field"
          },
          {
            "params": [],
            "type": "sum"
          }
        ]
      ],
      "tags": [
        {
          "condition": "AND",
          "key": "cluster::tag",
          "operator": "=~",
          "value": "/^$cluster$/"
        }
      ]
    }
  ],
  "datasource": {
    "type": "influxdb"
  },
  "options": {
    "tooltip": {
      "mode": "multi",
      "sort": "desc",
      "hideZeros": false
    },
    "legend": {
      "showLegend": true,
      "displayMode": "list",
      "placement": "bottom",
      "calcs": []
    }
  }
}

