{
  "type": "bargauge",
  "title": "Current percentage busy memory / host",
  "gridPos": {
    "x": 12,
    "y": 0,
    "h": 8,
    "w": 5
  },
  "fieldConfig": {
    "defaults": {
      "mappings": [],
      "thresholds": {
        "mode": "percentage",
        "steps": [
          {
            "color": "green",
            "value": null
          }
        ]
      },
      "color": {
        "mode": "continuous-GrYlRd"
      },
      "max": 1,
      "min": 0,
      "noValue": "-",
      "unit": "percentunit"
    },
    "overrides": []
  },
  "pluginVersion": "12.0.2",
  "targets": [
    {
      "alias": "$tag_host",
      "datasource": {
        "type": "influxdb"
      },
      "groupBy": [
        {
          "params": [
            "host::tag"
          ],
          "type": "tag"
        }
      ],
      "measurement": "fp-cpu-usage",
      "orderByTime": "ASC",
      "policy": "autogen",
      "query": "SELECT ((total - available) / total) FROM \"system-memory\" WHERE $timeFilter GROUP BY \"host\"::tag",
      "rawQuery": true,
      "refId": "A",
      "resultFormat": "time_series",
      "select": [
        [
          {
            "params": [
              "busy"
            ],
            "type": "field"
          }
        ]
      ]
    }
  ],
  "datasource": {
    "type": "influxdb"
  },
  "options": {
    "reduceOptions": {
      "values": false,
      "calcs": [
        "last"
      ],
      "fields": ""
    },
    "orientation": "horizontal",
    "legend": {
      "showLegend": false,
      "displayMode": "list",
      "placement": "bottom",
      "calcs": []
    },
    "displayMode": "gradient",
    "valueMode": "color",
    "namePlacement": "auto",
    "showUnfilled": true,
    "sizing": "auto",
    "minVizWidth": 0,
    "minVizHeight": 10,
    "maxVizHeight": 300,
    "text": {
      "titleSize": 18
    }
  }
}
