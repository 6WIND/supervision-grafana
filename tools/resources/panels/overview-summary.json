        "properties": [
          {
            "id": "displayName",
            "value": "License"
          },
          {
            "id": "mappings",
            "value": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 0,
                    "text": "invalid"
                  },
                  "1": {
                    "color": "green",
