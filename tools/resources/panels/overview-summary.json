          {
            "id": "mappings",
            "value": [
              {
                "options": {
                  "0": {
                    "color": "green",
                    "index": 0,
                    "text": "ok"
                  },
                  "1": {
                    "color": "orange",
                    "index": 1,
                    "text": "loaded"
                  },
                  "2": {
                    "color": "red",
                    "index": 2,
                    "text": "stopped"
                  },
                  "3": {
                    "color": "red",
                    "index": 3,
                    "text": "bad"
                  },
                  "null": {
                    "color": "green",
                    "index": 4,
                    "text": "N/A"
                  }
                },
                "type": "value"
              }
            ]
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "not-cleared"
        },
        "properties": [
          {
            "id": "displayName",
            "value": "Raised alarms"
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "product:version"
        },
        "properties": [
          {
            "id": "displayName",
            "value": "Version"
          }
        ]
      }
    ]
  },
